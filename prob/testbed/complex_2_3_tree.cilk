        printf("\t\t");
    }
    printf("[%d, %d] {%d} (%d) (?%d) %p", current_node->left_data,current_node->right_data, current_node->max, current_node->height,current_node->is_marked,current_node);
	printf("\n");
    
	if(current_node->middle != NULL){
		print_tree(current_node->middle, i+1);
    }
